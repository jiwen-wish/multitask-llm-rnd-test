name: "query_classify_tokenizer"
max_batch_size: 64
backend: "python"

input [
    {
        name: "text"
        data_type: TYPE_STRING
        dims: [ 1 ]
    }
]

output [
    {
        name: "input_ids"
        data_type: TYPE_INT64
        dims: [ 50 ] 
    },
    {
        name: "attention_mask"
        data_type: TYPE_INT64
        dims: [ 50 ]
    }
]

instance_group [
    {
      count: 1
      kind: KIND_CPU
    }
]